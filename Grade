#program for grade confirmation 
sno=int(input('enter sno'))
sname=input('enter student name')
group=input('enter student group')
s1=int(input('enter maths marks'))
s2=int(input('enter phy marks'))
s3=int(input('enter cs marks'))
total=s1+s2+s3
avg=total//3
if avg>=90:
    print('O grade')
elif avg>=80:
    print('A grade')
elif avg>=70:
    print('B grade')
elif avg>=60:
    print('C grade')
 elif avg>=50:
    print('D grade')
else:
    print('Pass')
